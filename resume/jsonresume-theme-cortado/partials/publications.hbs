{{#if resume.publications.length}}
<div class="container publications-container">
  {{> title value="Selected Publications"}}
    {{#each resume.publications}}
        <section class="item">
          <h5>
            {{#if authors_pre}}
            <span>{{authors_pre}}</span>,
            {{/if}}
            <span><strong>{{author_me}}</strong></span>,
            {{#if authors_post}}
            <span>{{authors_post}}</span>,
            {{/if}}
            <span class="journal-name">{{name}}</span>,
            <span>{{releaseDate}}</span>,
            <span>{{publisher}}</span>,
            <span>{{cc}}</span>
          </h5>
        </section>
    {{/each}}
</div>
{{/if}}
